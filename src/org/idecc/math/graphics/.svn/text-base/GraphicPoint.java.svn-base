package org.idecc.math.graphics;

public class GraphicPoint extends org.idecc.math.Point
{
	protected PointStyle _style;

	public GraphicPoint(double x, double y)
	{
		super(x,y);
	}
	public GraphicPoint()
	{
		this(0,0);
	}

	public void setStyle(PointStyle ps) { _style = ps; }
	public PointStyle getStyle() { return _style; }
}